<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/shopping_cart/db" VERSION="20220201" COMMENT="XMLDB file for Moodle local/shopping_cart" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="local_shopping_cart_history" COMMENT="Shopping Cart History">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
                <FIELD NAME="userid" TYPE="text" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="userfrom" TYPE="text" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="itemid" TYPE="text" NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="itemprice" TYPE="text"  NOTNULL="false" SEQUENCE="false" />
                <FIELD NAME="componentname" TYPE="text"  NOTNULL="false" SEQUENCE="false" />
				<FIELD NAME="timecreated" TYPE="int" LENGTH="10"  NOTNULL="false"  SEQUENCE="false" />
				<FIELD NAME="identifier" TYPE="text" LENGTH="20" NOTNULL="false"  SEQUENCE="false" />
				<FIELD NAME="payment" TYPE="text" LENGTH="10" NOTNULL="false" SEQUENCE="false" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
